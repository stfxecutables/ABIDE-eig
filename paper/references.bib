  shorttitle = {Disease Prediction Using Graph Convolutional Networks},
  author = {Parisot, Sarah and Ktena, Sofia Ira and Ferrante, Enzo and Lee, Matthew and Guerrero, Ricardo and Glocker, Ben and Rueckert, Daniel},
  year = {2018},
