  booktitle = {2020 {{IEEE International Conference}} on {{Big Data}} and {{Smart Computing}} ({{BigComp}})},
  author = {Byeon, Kyoungseob and Kwon, Junmo and Hong, Jisu and Park, Hyunjin},
